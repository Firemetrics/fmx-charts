          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH latest_encounter AS (\n  SELECT\n    es0._reference_id AS patient_id,\n    MAX(lower(ep0.start)) AS latest_encounter_start,\n    lower(p1.\"birthDate\") AS birth_date\n  FROM\n    encounter e0\n    JOIN encounter_period ep0 ON ep0._resource = e0._id\n    JOIN encounter_subject es0 ON es0._resource = e0._id\n    JOIN patient p1 ON (\n      p1._id = es0._reference_id\n      AND es0._reference_type = 'Patient'\n    )\n  GROUP BY\n    es0._reference_id, p1.\"birthDate\"\n)\nSELECT\n  AVG(EXTRACT(YEAR FROM AGE(latest_encounter_start, birth_date))) AS \"average_age\"\nFROM\n  latest_encounter;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
