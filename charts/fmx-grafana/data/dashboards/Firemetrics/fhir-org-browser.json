          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "select \n  o.id as fhir_id,\n  oi.value as \"ID\",\n  case when o.active then '✅' else '❌' end as \"Active\",\n  o.name as \"Name\",\n  string_agg(oa.value, ', ') as \"Alias\",\n  string_agg(distinct fmx_code(ot.system) || ': ' || ot.value, ', ') as \"Contact\"\nfrom organization o\njoin organization_identifier oi on (o._id = oi._parent)\nleft join organization_alias oa on (o._id = oa._parent)\nleft join organization_telecom ot on (o._id = ot._parent)\nwhere \n  case when ${org_sys_code:sqlstring} = 'all' then 1=1 else oi.system = ${org_sys_code} end\ngroup by 1,2,3,4\norder by 1;",
          "refId": "A",
