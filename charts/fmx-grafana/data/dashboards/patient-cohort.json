          "rawQuery": true,
          "rawSql": "with cohort as (\n  select \n    lower(p.\"birthDate\") birthdate,\n    lower(p.\"deceasedDateTime\") deceased_date,\n    fmx_code(p.gender) gender,\n    age(coalesce(lower(p.\"deceasedDateTime\"), now()), lower(p.\"birthDate\")) as age\n  from patient p\n  where exists (\n    select true from condition_code_coding ccc \n    join condition_subject cs on (ccc._resource = cs._resource and ccc.display in (${conditions}) and p._id = cs._reference_id and cs._reference_type = 'Patient')\n  )\n) select\n  count(*) \"Cohort Size\",\n    extract(year from avg(age)) \"Average age\",\n    extract(year from percentile_cont(0.5) within group (order by age)) \"Median Age\",\n    date_trunc('day', min(cohort.age)) \"Youngest age\",\n    max(cohort.birthdate)::text \"Youngest birthdate\",\n    date_trunc('day', max(cohort.age)) \"Oldest age\",\n    min(cohort.birthdate)::text \"Oldest birthdate\"\n  from cohort ",
          "refId": "A",
